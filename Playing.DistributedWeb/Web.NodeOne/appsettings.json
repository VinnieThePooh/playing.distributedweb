    "JAEGER_AGENT_PORT": 6831,
    "JAEGER_REPORTER_LOG_SPANS": false,
    "JAEGER_REPORTER_MAX_QUEUE_SIZE": 50,
    "JAEGER_SAMPLER_TYPE": "const",
    "JAEGER_TAGS": [
      "sampler.type",
      "sampler.param",
      "span.kind",
      "http.method",
      "http.url",
      "http.status_code",
      "component"
    ],
    "JAEGER_SENDER_FACTORY": "thrift" // current active sender factory (if multiple are registered)
  },
	"Kestrel":{
    	"Certificates":{
        	"Default":{
