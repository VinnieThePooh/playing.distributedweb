{	
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Messaging": {
    "Duration": 5,
    "Nodes": {
      "WebSocket": {
        "SocketUrl": "wss://localhost:5001/ws",
        "DisconnectRetryCount": 3
      }
    }
  },
  "ConnectionStrings": {
    "MariaDb": "Server=localhost;Port=3306;Uid=root;Database=distributed_web;Pwd="
  },
  "JaegerSettings": {
    "JAEGER_SERVICE_NAME": "Web.NodeOne Service",
