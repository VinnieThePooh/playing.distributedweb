{
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "WebSocketServer": {
    "AllowedOrigins": "*",
    "KeepAliveInterval": "120",
    "SocketPath": "/ws"
  },
  "KafkaOptions": {
    "ClientId": "Web.NodeTwo",
    "BootstrapServerUrl": "localhost:9092",
    "TopicName": "distributed-web"
  },
  "JaegerSettings": {
    "JAEGER_SERVICE_NAME": "Web.NodeTwo Service",
    "JAEGER_ENDPOINT": "http://localhost:9411/api/traces",
    //"JAEGER_REPORTER_LOG_SPANS": "what is it?",
    "JAEGER_REPORTER_MAX_QUEUE_SIZE": 50,
    "JAEGER_SAMPLER_TYPE": "const",
    //"JAEGER_TAGS": "",
    "JAEGER_SENDER_FACTORY": "ThriftSenderFactory"
  }
}
